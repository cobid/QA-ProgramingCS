int num = int.Parse(Console.ReadLine());

for (int i = 1; i <= num; i++)
{
    for (int j = 0; j <= num; j++)
    {
        for (int k = 0; k <= num; k++)
        {
            for (int m = 0; m <= num; m++)
            {
                if (i + j == k + m && k + m == num)
                {
                    Console.Write($"{i}{j}{k}{m} ");
                }
            }
        }
    }
}
