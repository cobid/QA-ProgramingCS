int num = int.Parse(Console.ReadLine());
int currentDigit = 0;
int oddDigit = 0;
int evenDigit = 0;

for (int i = 2; i <= num; i += 2)
{
        evenDigit = i;
    
    for (int j = 1; j <= num; j += 2)
    {
        oddDigit = j;
        Console.Write($"{evenDigit}{oddDigit}{evenDigit * oddDigit} ");
    } 
}
