string input = Console.ReadLine().ToUpper();

VowelsCount(input);

static void VowelsCount(string input)
{
    int countVowels = 0;

    for (int i = 0; i < input.Length; i++)
    {
        if (input[i] == 'A' || input[i] == 'I' || input[i] == 'E' || input[i] == 'O' || input[i] == 'U')
        {
            countVowels++;
        }
    }
    Console.WriteLine(countVowels);
}
