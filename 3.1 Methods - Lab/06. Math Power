int num = int.Parse(Console.ReadLine());
int pow = int.Parse(Console.ReadLine());

double result = MathPower(num, pow);
Console.WriteLine(result);
static double MathPower(int num, int pow)
{
    double result = 1;
    for (int i = 1; i <= pow; i++)
    {
        result *= num;
    }
    return result;
}
