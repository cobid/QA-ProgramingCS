int firstNum = int.Parse(Console.ReadLine());
int secondNum = int.Parse(Console.ReadLine());

int resultFromFirstNum = FactorialFromInteger(firstNum);
int resultFromSecondNum = FactorialFromInteger(secondNum);
Console.WriteLine(resultFromFirstNum / resultFromSecondNum);

static int FactorialFromInteger(int num)
{
    int result = 1;

    for (int i = 1; i <= num; i++)
    {
        result *= i;
    }

    return result;
}
