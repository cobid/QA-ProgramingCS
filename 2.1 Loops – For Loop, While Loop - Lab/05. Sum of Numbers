namespace _05._Sum_of_Numbers
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int num = int.Parse(Console.ReadLine());
            int sum = 0;

            while (sum < num)
            {
                int nextNum = int.Parse(Console.ReadLine());
                sum = sum + nextNum;    
                
            }
             Console.WriteLine(sum);
        }
    }
}
