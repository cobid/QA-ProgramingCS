namespace _06._Sequence_2k_1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int num = int.Parse(Console.ReadLine());
            int k = 0;
            int doubK = 1;

            while (num >= doubK)
            {
                Console.WriteLine(doubK);
                k = doubK;
                doubK = 2 * k + 1;
                               
            }
            
        }
    }
}
