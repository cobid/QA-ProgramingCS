string name = Console.ReadLine();
while (name != "end")
{

    List<char> result = new List<char>();

    for (int i = name.Length - 1; i >= 0; i--)
    {
        char c = name[i];
        result.Add(c);
        
    }
    Console.WriteLine("{0} = " + String.Join("", result), name);
    name = Console.ReadLine();
}
